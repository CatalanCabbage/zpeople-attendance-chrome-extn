{
    "manifest_version": 2,
  
    "name": "Zoho WFH Availability",
    "description": "This extension will enable you to Check-In or Check-Out in Zoho People",
    "version": "1.0",
    "background": {
        "scripts": ["background.js"],
        "persistent": false
      },
    "browser_action": {
     "default_icon": "ZPeople-icon.png",
     "default_title": "Check current Availability"
    },
    "permissions": [
     "cookies",
     "activeTab",
     "declarativeContent",
     "*://*.zoho.com/*"
     ]
  }